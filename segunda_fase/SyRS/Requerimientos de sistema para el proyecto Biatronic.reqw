            {
              "id": "4",
              "guid": "7c2e38bd-b2da-487a-8e34-52a03d0c45c5",
              "heading": "Visión general del sistema",
              "helpRef": "9.5.4",
              "type": "SEC",
              "history": [
                {
                  "attributesChanged": {
                    "heading": "System overview"
                  },
                  "changedBy": {
                    "company": "",
                    "email": "ocramsalazar@gmail.com",
                    "name": "Marco Zolla"
